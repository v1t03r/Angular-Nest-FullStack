{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/api-interfaces/tsconfig.lib.json"],
            "exclude": ["**/node_modules/**", "!libs/api-interfaces/**/*"]
          }
        }
      },
      "tags": []
    },
    "boiler": {
      "projectType": "application",
      "root": "apps/boiler",
      "sourceRoot": "apps/boiler/src",
      "prefix": "ng-boiler",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/boiler",
            "index": "apps/boiler/src/index.html",
            "main": "apps/boiler/src/main.ts",
            "polyfills": "apps/boiler/src/polyfills.ts",
            "tsConfig": "apps/boiler/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/boiler/src/favicon.ico", "apps/boiler/src/assets"],
            "styles": ["apps/boiler/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/boiler/src/environments/environment.ts",
                  "with": "apps/boiler/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "boiler:build:production"
            },
            "development": {
              "browserTarget": "boiler:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "apps/boiler/proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "boiler:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/boiler/src/**/*.ts",
              "apps/boiler/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/boiler"],
          "options": {
            "jestConfig": "apps/boiler/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "boiler-e2e": {
      "root": "apps/boiler-e2e",
      "sourceRoot": "apps/boiler-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/boiler-e2e/cypress.json",
            "devServerTarget": "boiler:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "boiler:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/boiler-e2e/tsconfig.json"],
            "exclude": ["**/node_modules/**", "!apps/boiler-e2e/**/*"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["boiler"]
    }
  }
}
